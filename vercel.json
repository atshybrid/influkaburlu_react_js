{
  "routes": [
    { "src": "/influencers/?$", "status": 301, "headers": { "Location": "/creators" } },
    { "src": "/influencer/(.*)", "status": 301, "headers": { "Location": "/creators/$1" } },
    { "src": "/register/brand/?$", "status": 301, "headers": { "Location": "/brand/register" } },
    { "handle": "filesystem" },
    { "src": "/sitemap\\.xml", "dest": "/api/sitemap.xml" },
    { "src": "/robots\\.txt", "dest": "/api/robots.txt" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
